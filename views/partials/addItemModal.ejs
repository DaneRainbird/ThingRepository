<div class="modal fade" id="addItemModel" tabindex="-1" aria-labelledby="addItemModel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="addItemModel">Add Thing</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
            </div>
            <div class="modal-body">
                <form method="POST" autocomplete="off" action="/addThing" class="modal-input">
                    <b>Name: </b><input type="text" name="name" value="" required>
                    <b>Description: </b><textarea name="description" value="" required></textarea>
                    <b>Price: </b><input type="number" name="price" value="" required>
                    <b>Date: </b><input type="date" name="date" value="" required>
                    <b>Image: </b><input type="file" name="image" value = "">
                    <button type="submit" class="btn btn-primary">Add Thing</button>
                </form>
            </div>
        </div>
    </div>
</div>